<!DOCTYPE html>
<html lang="en">
<%- include("../partials/head.ejs") %> 
<body>
    <%- include("../partials/header.ejs") %> 
    <main>
        <div>
            <div>
                <a href="/vehicle/new"><button>Add a vehicle</button></a>
            </div>
            <% for (vehicle of vehicles) { %>
                <a href="/vehicle/<%= vehicle._id %>">
                    <h2>
                        <%= vehicle.company %> - <%= vehicle.inStock ? "Yes" : "No" %>  
                    </h2>
                </a>
            <% } %> 
        </div>
        <%- include("../partials/footer.ejs") %> 
    </main>
</body>
</html>